<?xml version="1.0" encoding="utf-8"?>
<CatapultSkin
  Version="2"
  SkinType="Component"
  Comment="HTML5 skin"
  Anchors="Width,Height"
  Width="800"
  Height="600"
  Top="0"
  Left="0"
  Bottom="0"
  Right="0"
  Tabs="TOC,Index,Community"
  DefaultTab="TOC"
  UseBrowserDefaultSize="True"
  UseDefaultBrowserSetup="True"
  EnableResponsiveOutput="True"
  ComponentType="SearchBar">
  <Stylesheet>
    <Styles>
      <Style
        Name="Search Bar">
        <Properties>
          <Property
            Name="Width">700px</Property>
          <Property
            Name="BorderTopLeftRadius">5px</Property>
          <Property
            Name="BorderTopRightRadius">5px</Property>
          <Property
            Name="BorderBottomRightRadius">5px</Property>
          <Property
            Name="BorderBottomLeftRadius">5px</Property>
          <Property
            Name="Height">40px</Property>
          <Property
            Name="BorderTop">solid 0px #999999</Property>
          <Property
            Name="BorderRight">solid 0px #999999</Property>
          <Property
            Name="BorderBottom">solid 0px #999999</Property>
          <Property
            Name="BorderLeft">solid 0px #999999</Property>
        </Properties>
      </Style>
      <Medium
        Name="Mobile">
        <Style
          Name="Search Bar">
          <Properties>
            <Property
              Name="Width">90%</Property>
          </Properties>
        </Style>
      </Medium>
      <Style
        Name="Search Filter">
        <Properties>
          <Property
            Name="BorderRight">solid 1px #a9a9a9</Property>
          <Property
            Name="BorderLeft">solid 1px #a9a9a9</Property>
        </Properties>
      </Style>
      <Style
        Name="Search Submit Button">
        <Properties>
          <Property
            Name="BackgroundColor">#ffffff</Property>
          <Property
            Name="Width">50px</Property>
          <Property
            Name="BackgroundImage">url('Icon_Search_20x20.png')</Property>
        </Properties>
      </Style>
      <Style
        Name="Search Input">
        <Properties>
          <Property
            Name="Placeholder">Search for help articles, videos, guides...</Property>
          <Property
            Name="Color">#333333</Property>
          <Property
            Name="FontSize">16px</Property>
        </Properties>
      </Style>
    </Styles>
    <Resources>
      <Resource
        Name="Icon_Search_20x20.png"></Resource>
    </Resources>
  </Stylesheet>
  <Strings>
    <Stylesheet
      lang="en-us">
      <Stylesheet>
        <Styles>
          <Style
            Name="Search Bar">
            <Properties>
              <Property
                Name="Height">50px</Property>
              <Property
                Name="BorderTopLeftRadius">10px</Property>
              <Property
                Name="BorderTopRightRadius">10px</Property>
              <Property
                Name="BorderBottomRightRadius">10px</Property>
              <Property
                Name="BorderBottomLeftRadius">10px</Property>
              <Property
                Name="BorderTop">solid 1px #a9a9a9</Property>
              <Property
                Name="BorderRight">solid 1px #a9a9a9</Property>
              <Property
                Name="BorderBottom">solid 1px #a9a9a9</Property>
              <Property
                Name="BorderLeft">solid 1px #a9a9a9</Property>
              <Property
                Name="Width">100%</Property>
            </Properties>
          </Style>
          <Medium
            Name="Mobile">
            <Style
              Name="Search Bar">
              <Properties>
                <Property
                  Name="Width">100%</Property>
              </Properties>
            </Style>
          </Medium>
          <Style
            Name="Search Filter">
            <Properties>
              <Property
                Name="BorderRight">solid 1px #a9a9a9</Property>
              <Property
                Name="BorderLeft">solid 1px #a9a9a9</Property>
            </Properties>
          </Style>
          <Style
            Name="Search Submit Button">
            <Properties>
              <Property
                Name="BackgroundColor">#ffffff</Property>
              <Property
                Name="Width">50px</Property>
            </Properties>
          </Style>
          <Style
            Name="Search Input">
            <Properties>
              <Property
                Name="Placeholder">Search for help articles, videos, guides...</Property>
            </Properties>
          </Style>
        </Styles>
      </Stylesheet>
    </Stylesheet>
  </Strings>
</CatapultSkin>